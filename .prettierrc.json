{
  "$schema": "http://json.schemastore.org/prettierrc",
  "useTabs": false,
  "tabWidth": 2,
  "printWidth": 100,
  "proseWrap": "preserve",
  "singleQuote": true,
  "semi": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "quoteProps": "consistent"
}
